    write /sys/devices/system/cpu/eas/enable 2
    write /proc/cpufreq/cpufreq_sched_disable 0
    chown system system /dev/stune/foreground/schedtune.prefer_idle
    write /dev/stune/foreground/schedtune.prefer_idle 0
    write /dev/stune/top-app/schedtune.prefer_idle 0
    write /dev/stune/rt/schedtune.prefer_idle 0
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "schedutil"
    write /sys/devices/system/cpu/cpu0/cpufreq/sched/down_throttle_nsec 20000000
    write /sys/devices/system/cpu/cpufreq/schedutil/up_rate_limit_us 10000
    write /sys/devices/system/cpu/cpufreq/schedutil/down_rate_limit_us 10000

    # restore perf. parameters
    write /proc/perfmgr/boost_ctrl/eas_ctrl/boot_boost "0 20"
    write /proc/perfmgr/boost_ctrl/eas_ctrl/boot_boost "1 20"
    write /proc/perfmgr/boost_ctrl/eas_ctrl/boot_boost "2 20"
    write /proc/perfmgr/boost_ctrl/eas_ctrl/boot_boost "3 20"
    write /proc/perfmgr/boost_ctrl/eas_ctrl/boot_boost "4 20"
    write /proc/perfmgr/boost_ctrl/cpu_ctrl/boot_freq "-1 -1 -1 -1"
    #restore vcore parameters
    write /sys/power/vcorefs/vcore_debug "KIR_BOOTUP -1"

# stop bootprof
on property:sys.boot_completed=1
    write /proc/bootprof 0
    write /proc/mtprintk ${persist.vendor.uartconsole.enable}

